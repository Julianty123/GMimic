<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="201.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GMimic">
    <Accordion layoutX="318.0" layoutY="77.0" />
    <Pane layoutX="20.0" layoutY="16.0" prefHeight="169.0" prefWidth="351.0" style="-fx-border-color: #888888; -fx-border-radius: 25px;">
        <CheckBox fx:id="checkTile" layoutX="167.0" layoutY="79.0" mnemonicParsing="false" onAction="#handlecheckTile" text="Follow Tile (Beta)" />
        <CheckBox fx:id="checkLook" layoutX="174.0" layoutY="43.0" mnemonicParsing="false" onAction="#handlecheckLook" text="Mimic Look" />
        <CheckBox fx:id="checkSpeech" layoutX="23.0" layoutY="43.0" mnemonicParsing="false" onAction="#handleCheckSpeech" text="Mimic Speech" />
        <CheckBox fx:id="checkName" layoutX="23.0" layoutY="11.0" mnemonicParsing="false" selected="true" text="Click the user to mimic" />
        <CheckBox fx:id="checkBot" layoutX="209.0" layoutY="124.0" mnemonicParsing="false" onAction="#handlecheckBot" text="Mimic Bot" />
        <CheckBox fx:id="checkMotto" layoutX="23.0" layoutY="79.0" mnemonicParsing="false" onAction="#handlecheckMotto" text="Mimic Motto" />
        <Label layoutX="47.0" layoutY="124.0" text="Delay:" />
        <TextField fx:id="textFieldDelay" alignment="CENTER" layoutX="92.0" layoutY="119.0" prefHeight="31.0" prefWidth="45.0" text="1" />
    </Pane>
</AnchorPane>
